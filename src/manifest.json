{
    "manifest_version": 2,
    "name": "Socket.IO Extension",
    "description": "An extension that uses Socket.IO to communicate with a server.",
    "version": "1.0",
    "permissions": ["http://localhost/*"],
    "background": {
      "scripts": ["background.js"]
    },
    "browser_action": {
      "default_popup": "popup.html"
    }
  }
  